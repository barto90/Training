{
  "name": "jwks-client",
  "version": "1.4.4",
  "description": "Library to retrieve public keys from a JWKS endpoint",
  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "lines": 99,
    "statements": 99,
    "functions": 100,
    "branches": 90,
    "exclude": [
      "./tests/**"
    ],
    "reporter": [
      "json",
      "lcov",
      "text-summary"
    ]
  },
  "dependencies": {
    "debug": "^4.3.3",
    "jwk-to-pem": "^2.0.6",
    "limiter": "^1.1.5",
    "lru-memoizer": "^2.1.4",
    "ms": "^2.1.3",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "^8.7.0",
    "mocha": "^10.1.0",
    "nock": "^13.2.2",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.1"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "NODE_ENV=test nyc mocha --timeout 5000 $(find ./tests -name *.tests.js)",
    "test-watch": "NODE_ENV=test mocha --timeout 5000 $(find ./tests -name *.tests.js) --watch",
    "release": "git tag $npm_package_version && git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/JacoKoster/node-jwks-client.git"
  },
  "keywords": [
    "jwks",
    "jwt",
    "rsa",
    "ecdsa"
  ],
  "author": "JacoKoster",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/JacoKoster/node-jwks-client/issues"
  },
  "homepage": "https://gitlab.com/JacoKoster/node-jwks-client"
}
